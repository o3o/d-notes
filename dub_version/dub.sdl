name "dub_version"
version "~master"
description "Test dub versions flag"
authors "o3o"
copyright "Copyright Â© 2016, o3o"
license "proprietary"
sourcePaths "."
configuration "exec" {
	targetType "executable"
}

configuration "x" {
	targetType "executable"
	versions "x"
}

/*
Si possono specificare piu' versioni in questo modo:
	versions "x" "y"
Per compilare
dub build -cxy
*/
configuration "xy" {
	targetType "executable"
	versions "x" "y"
}
